<?xml version="1.0" encoding="UTF-8"?>
<actions>
	<action>
		<icon>Terminal</icon>
		<name>Terminal</name>
		<unique-id>Action_Terminal-010</unique-id>
		<command>exo-open --working-directory %f --launch TerminalEmulator</command>
		<description></description>
		<patterns>*</patterns>
		<directories/>
	</action>
	<action>
		<icon>sublime-text</icon>
		<name>Sublime Text</name>
		<unique-id>Action_Text_Editor-010</unique-id>
		<command>/usr/bin/subl %F</command>
		<description></description>
		<patterns>*</patterns>
		<directories/>
		<other-files/>
		<text-files/>
	</action>
	<action>
		<icon>kr_comparedirs</icon>
		<name>Compare</name>
		<unique-id>Action_File_Compare-010</unique-id>
		<command>meld %F</command>
		<description>Compare files and directories with meld</description>
		<patterns>*</patterns>
		<directories/>
		<text-files/>
	</action>
	<action>
		<icon>Terminal</icon>
		<name>Xftt</name>
		<unique-id>Action_Terminal-020</unique-id>
		<command>xftt %f</command>
		<description></description>
		<patterns>*</patterns>
		<directories/>
		<other-files/>
		<text-files/>
		<image-files/>
		<video-files/>
		<audio-files/>
	</action>
	<action>
		<icon>atom</icon>
		<name>Atom</name>
		<unique-id>Action_Text_Editor-020</unique-id>
		<command>env ATOM_DISABLE_SHELLING_OUT_FOR_ENVIRONMENT=false /usr/bin/atom %F</command>
		<description></description>
		<patterns>*</patterns>
		<directories/>
		<other-files/>
		<text-files/>
	</action>
	<action>
		<icon>micro</icon>
		<name>xftmicro</name>
		<unique-id>Action_Text_Editor-030</unique-id>
		<command>xftmicro %F</command>
		<description></description>
		<patterns>*</patterns>
		<other-files/>
		<text-files/>
	</action>
	<action>
		<icon>nvim</icon>
		<name>xftnvim</name>
		<unique-id>Action_Text_Editor-040</unique-id>
		<command>xftnvim %F</command>
		<description></description>
		<patterns>*</patterns>
		<directories/>
		<other-files/>
		<text-files/>
	</action>
	<action>
		<icon>preferences-desktop-wallpaper</icon>
		<name>Set as Wallpaper by Feh</name>
		<unique-id>Action_Wallpaper_Select-010</unique-id>
		<command>opset-wallpaper-feh %f</command>
		<description></description>
		<patterns>*</patterns>
		<image-files/>
	</action>
	<action>
		<icon>stock_folder</icon>
		<name>Thunar Root</name>
		<unique-id>Action_File_Manager-010</unique-id>
		<command>pkexec thunar %f</command>
		<description>Thunar Root</description>
		<patterns>*</patterns>
		<directories/>
	</action>
	<action>
		<icon>system-search</icon>
		<name>Search</name>
		<unique-id>Action_File_Finder-010</unique-id>
		<command>catfish %f</command>
		<description>find files and folders</description>
		<patterns>*</patterns>
		<directories/>
	</action>
