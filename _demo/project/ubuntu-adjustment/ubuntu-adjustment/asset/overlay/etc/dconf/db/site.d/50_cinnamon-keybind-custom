

[org/cinnamon/desktop/keybindings]
custom-list=['__dummy__', 'system-logout', 'system-shutdown', 'wallpaper-shuf', 'wallpaper-default', 'system-settings', 'rofi-show-run', 'rofi-show-window', 'rofi-show-drun', 'terminal', 'terminal-1', 'terminal-2', 'terminal-3', 'terminal-4', 'text-editor', 'web-browser', 'file-manager', 'file-manager-1', 'volume-control', 'volume-toggle-mute', 'volume-decrease', 'volume-increase', 'volume-decrease-slowly', 'volume-increase-slowly']




[org/cinnamon/desktop/keybindings/custom-keybindings/file-manager-1]
binding=['<Shift><Alt>g']
command='thunar'
name='File_Manager-1'

[org/cinnamon/desktop/keybindings/custom-keybindings/file-manager]
binding=['<Shift><Alt>f']
command='pcmanfm-qt'
name='File_Manager'

[org/cinnamon/desktop/keybindings/custom-keybindings/rofi-show-drun]
binding=['<Shift><Alt>d']
command='rofi -show drun -show-icons'
name='Rofi_Show_Drun'

[org/cinnamon/desktop/keybindings/custom-keybindings/rofi-show-run]
binding=['<Shift><Alt>r']
command='rofi -show run'
name='Rofi_Show_Run'

[org/cinnamon/desktop/keybindings/custom-keybindings/rofi-show-window]
binding=['<Shift><Alt>w']
command='rofi -show window -show-icons'
name='Rofi_Show_Window'

[org/cinnamon/desktop/keybindings/custom-keybindings/system-settings]
binding=['<Shift><Alt>s']
command='cinnamon-settings'
name='System_Settings'

[org/cinnamon/desktop/keybindings/custom-keybindings/system-logout]
binding=['<Shift><Alt>x']
command='cinnamon-session-quit --logout'
name='System_Logout'

[org/cinnamon/desktop/keybindings/custom-keybindings/system-shutdown]
binding=['<Shift><Alt>z']
command='cinnamon-session-quit --power-off'
name='System_Shutdown'

[org/cinnamon/desktop/keybindings/custom-keybindings/terminal-1]
binding=['<Shift><Alt>a']
command='sakura -m'
name='Terminal-1'

[org/cinnamon/desktop/keybindings/custom-keybindings/terminal-2]
binding=['<Control><Alt>a']
command='xfce4-terminal --maximize'
name='Terminal-2'

[org/cinnamon/desktop/keybindings/custom-keybindings/terminal-3]
binding=['<Shift><Alt>t']
command='xterm'
name='Terminal-3'

[org/cinnamon/desktop/keybindings/custom-keybindings/terminal-4]
binding=['<Control><Alt>t']
command='urxvt'
name='Terminal-4'

[org/cinnamon/desktop/keybindings/custom-keybindings/terminal]
binding=['<Alt>Return']
command='sakura -m'
name='Terminal'

[org/cinnamon/desktop/keybindings/custom-keybindings/text-editor]
binding=['<Shift><Alt>e']
command='mousepad'
name='Text_Editor'

[org/cinnamon/desktop/keybindings/custom-keybindings/volume-control]
binding=['<Shift><Alt>v']
command='mate-volume-control'
name='Volume_Control'

[org/cinnamon/desktop/keybindings/custom-keybindings/volume-decrease-slowly]
binding=['<Control><Alt>comma']
command='amixer -q -D pulse sset Master 1%- unmute'
name='Volume_Decrease_Slowly'

[org/cinnamon/desktop/keybindings/custom-keybindings/volume-decrease]
binding=['<Shift><Alt>comma']
command='amixer -q -D pulse sset Master 5%- unmute'
name='Volume_Decrease'

[org/cinnamon/desktop/keybindings/custom-keybindings/volume-increase-slowly]
binding=['<Control><Alt>period']
command='amixer -q -D pulse sset Master 1%+ unmute'
name='Volume_Increase_Slowly'

[org/cinnamon/desktop/keybindings/custom-keybindings/volume-increase]
binding=['<Shift><Alt>period']
command='amixer -q -D pulse sset Master 5%+ unmute'
name='Volume_Decrease'

[org/cinnamon/desktop/keybindings/custom-keybindings/volume-toggle-mute]
binding=['<Alt>m']
command='amixer -q -D pulse sset Master toggle'
name='Volume_Toggle_Mute'

[org/cinnamon/desktop/keybindings/custom-keybindings/wallpaper-default]
binding=['<Control><Alt>w']
command='wallpaper-select-gnome default'
name='Wallpaper_Default'

[org/cinnamon/desktop/keybindings/custom-keybindings/wallpaper-shuf]
binding=['<Alt>w']
command='wallpaper-select-gnome shuf_favorite'
name='Wallpaper_Shuf'

[org/cinnamon/desktop/keybindings/custom-keybindings/web-browser]
binding=['<Shift><Alt>b']
command='falkon --new-tab about:blank'
name='Web_Browser'
